{% extends 'SettingsConfigBundle::layout.html.twig' %}

{% block title %}Test Procedure Listing{% endblock%}

{% block main %}
    {{ parent() }}
    <div class="cont font-check">
    <h4 class="desc-cont">TEST PROCEDURES CONFIGURATION</h4> 
    {% include 'SettingsConfigBundle:TestProcedure:list.html.twig' with { 'procedures': procedures } %}
    <div class = "form_container">
    <h3>Edit Test Procedure</h3>
    {% render controller('SettingsConfigBundle:TestProcedure:create', {'id' : procedure_id}) %}
    </div>
</div>
<div style="padding-bottom:40px;clear:both;"></div>
{% endblock %}