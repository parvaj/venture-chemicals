{% extends '::venture.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link href='http://fonts.googleapis.com/css?family=Varela' rel='stylesheet' type='text/css'>
    {% stylesheets
    '@SettingsConfigBundle/Resources/public/css/common.css'
    %}
    <link href="{{ asset_url }}" rel="stylesheet" media="screen" />

    {% endstylesheets %}


{% endblock %}


{% block middle %}
    {{ parent() }}
    <div class = "maincont">
        {% block main %}
            {% for flashMessage in app.session.flashbag.get('notice') %}
                <div class="notice"> {{ flashMessage }} </div>
            {% endfor %}
        {% endblock %}
    </div>
{% endblock %}

{% block footmenu2 %}
    <li class="activeMenu"><a href="{{ path('SettingsConfigBundle_homepage') }}">Configuration</a></li>
{% endblock %}
