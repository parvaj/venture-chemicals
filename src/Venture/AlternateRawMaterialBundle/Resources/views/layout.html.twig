{% extends '::venture.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets
        '@VentureAlternateRawMaterialBundle/Resources/public/css/common.css'
        '@VentureAlternateRawMaterialBundle/Resources/public/css/form.css'
    %}
        <link href="{{ asset_url }}" rel="stylesheet" media="screen" />    
    {% endstylesheets %}
    <link href='http://fonts.googleapis.com/css?family=Varela' rel='stylesheet' type='text/css'>
{% endblock %}
        
{% block javascripts %}
    {{ parent() }}
    <script src="http://code.jquery.com/jquery-latest.min.js" type="text/javascript"></script>
    {% javascripts 
        '@VentureAlternateRawMaterialBundle/Resources/public/js/common_lib.js' 
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}


{% block middle %}
    {{ parent() }}
    <div class = "maincont">
        {% block main %} 
            {% for flashMessage in app.session.flashbag.get('notice') %}
                <div class="notice"> {{ flashMessage }} </div>
            {% endfor %}
        {% endblock %} 
   </div>    
{% endblock %}
        
{% block menu14 %}
        <li class="activeMenu"><a href="{{ path('venture_list_alternate_raw_material', {'type': 'show_all'}) }}">Alternate Raw Materials</a></li>
{% endblock %}
