{% extends '::venture.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
	<link href='http://fonts.googleapis.com/css?family=Varela' rel='stylesheet' type='text/css'>
    {% stylesheets
        '@SettingsConfigBundle/Resources/public/css/common.css'
        '@VentureUserBundle/Resources/public/css/user.css'
    %}
        <link href="{{ asset_url }}" rel="stylesheet" media="screen" />
		
    {% endstylesheets %}


{% endblock %}


{% block middle %}
    {{ parent() }}
       
    <div class = "maincont"> 
        {% block main %} 
            {% for type, messages in app.session.flashBag.all %}
                {% for message in messages %}
                    <div class="{{ type }} notice">
                        {{ message|trans({}, 'FOSUserBundle') }}
                    </div>
                {% endfor %}
            {% endfor %}
        {% endblock %}
    </div>
{% endblock %}
        
{% block footmenu3 %}
    <li class="activeMenu"><a href="{{ path('venture_users_list') }}">User</a></li>
{% endblock %}        