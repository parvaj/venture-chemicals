{{ form_start(form) }}
            
            <div class="property-cont">
                <div class= "form-element-controller">
                    <p class="label">{{ form_label(form.itemName) }}</p>
                    <p class="field">{{ form_widget(form.itemName) }}</p>
                    <p class="error">{{ form_errors(form.itemName) }}</p>
                </div>
                <div class= "form-element-controller">
                    <p class="label">{{ form_label(form.itemDescription) }}</p>
                    <p class="field">{{ form_widget(form.itemDescription) }}</p>
                    <p class="error">{{ form_errors(form.itemDescription) }}</p>
                </div>
                
                <div class= "form-element-controller">
                    <p class="label">{{ form_label(form.itemSupplier) }}</p>
                    <p class="field">{{ form_widget(form.itemSupplier) }}</p>
                    <p class="error">{{ form_errors(form.itemSupplier) }}</p>
                </div>
            </div>
            
            <div class="property-cont">
                <div class= "form-element-controller">
                    <p class="label">{{ form_label(form.configPackaging) }}</p>
                    <p class="field">{{ form_widget(form.configPackaging) }}</p>
                    <p class="error">{{ form_errors(form.configPackaging) }}</p>
                </div>
                <div class= "form-element-controller">
                    <p class="label">{{ form_label(form.tags) }}</p>
                    <p class="field">{{ form_widget(form.tags) }}</p>
                    <p class="error">{{ form_errors(form.tags) }}</p>
                </div>
                <div class= "form-element-controller">
                    <p class="label">{{ form_label(form.standardCost) }}</p>
                    <p class="field">{{ form_widget(form.standardCost) }}</p>
                    <p class="error">{{ form_errors(form.standardCost) }}</p>
                </div>
            </div>
            
            
            
            <div class="property-cont">
                <ul class="spec" data-prototype="{{ form_widget(form.properties.vars.prototype)|e }}">
                    {% for prop in form.properties %}
                    <li>
                        <div>
                            <div>
                                {{ form_label(prop.property) }}
                                {{ form_widget(prop.property) }}
                                {{ form_errors(prop.property) }}
                            </div>
                            
                            <div>
                                {{ form_label(prop.specificationMath) }}
                                {{ form_widget(prop.specificationMath) }}
                                {{ form_errors(prop.specificationMath) }}
                            </div>
                                
                            <div>
                                {{ form_label(prop.specification) }}
                                {{ form_widget(prop.specification) }}
                                {{ form_errors(prop.specification) }}
                            </div>
                                
                            <div>
                                {{ form_label(prop.testProcedure) }}
                                {{ form_widget(prop.testProcedure) }}
                                {{ form_errors(prop.testProcedure) }}
                            </div>
                        </div>
                        <a class="del_prop deleteParentElement" href="#">Delete</a>
                    </li>
                    {% endfor %}
                </ul>
            </div>
            
            <div class="property-cont">
                <div class="rows">
                    <div class= "form-element-controller four-items">
                        <p class="label">{{ form_label(form.isActive) }}</p>
                        <p class="field">{{ form_widget(form.isActive) }}</p>
                        <p class="error">{{ form_errors(form.isActive) }}</p>
                    </div>
                </div>
            </div>
            
            <div class="property-cont no-brd">
                <div class="rows">
                    <div class= "form-element-controller four-items">
                        <p class="field">
                            <button class="add-new" type="submit">Save</button>
                        </p>
                    </div>
                </div>
            </div>
        
       {{ form_end(form) }}